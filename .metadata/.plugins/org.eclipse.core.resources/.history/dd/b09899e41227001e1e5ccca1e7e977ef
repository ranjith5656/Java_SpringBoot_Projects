package com.bookstore.inventory.entity;

import java.util.List;

public class Order {

	private Long id;
    private List<Book> books;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public List<Book> getBooks() {
		return books;
	}
	public void setBooks(List<Book> books) {
		this.books = books;
	}
	public Order(Long id, List<Book> books) {
		super();
		this.id = id;
		this.books = books;
	}
	public Order() {
		super();
	}
    
    
}
